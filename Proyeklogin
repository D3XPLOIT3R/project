cat << LOGIN> bash.bashrc
perangkap '' 2
echo -e "\ e [1; 32m
                ──▄▀▀▀▄───────────────
                Please Login To Continue Your Termux
                ──█───█───────────────
                ─███████─────────▄▀▀▄─
                ░██─▀─██░░█▀█▀▀▀▀█░░█░
                ░███▄███░░▀░▀░░░░░▀▀░░
\ e [0m "
baca -p $ '\ e [32mUSERNAME: \ e [0m' pengguna
read -s -p $ '\ e [32mPASSWORD: \ e [0m' pass
if [[\ $ pass == $ password && \ $ user == $ username]]; kemudian
PS1 = '\ 033 [1; 32m
\ a┏ \ a \ 033 [1; 32m
\ a┏ \ a━ \ a┃ Terminal Profesional Anda - ┃ \ @ ┃ --- | \ d ┃
\ a \ a┃ \ a \ a \ 033 [1; 32jt $ pengguna ━┛ \ 033 [1; 36m
\ a┗ \ a┳ \ a━ \ a📂 \ 033 [1; 32m \ w \ a📂
┗ DEXPL0IT3R@ '
shopt -s autocd
